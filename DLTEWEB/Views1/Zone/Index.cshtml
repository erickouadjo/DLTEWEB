    <style>
        .dt-button {
            background-color: #28a745;
            border-color: #28a745;
            color: white;
            border-radius: 5px;
            padding: 8px 16px;
            margin: 5px;
        }

            .dt-button:hover {
                background-color: #218838;
                border-color: #1e7e34;
            }

        /* Styles pour le bouton PDF */
        .buttons-pdf {
            background-color: #dc3545;
            border-color: #dc3545;
            color: white;
        }

            .buttons-pdf:hover {
                background-color: #c82333;
                border-color: #bd2130;
            }

        /* Styles pour le bouton Excel */
        .buttons-excel {
            background-color: #28a745;
            border-color: #28a745;
            color: white;
        }

            .buttons-excel:hover {
                background-color: #218838;
                border-color: #1e7e34;
            }

        /* Styles pour le bouton Copy */
        .buttons-copy {
            background-color: #28a745;
            border-color: #28a745;
            color: white;
        }

            .buttons-copy:hover {
                background-color: #218838;
                border-color: #1e7e34;
            }

        .grid-container {
            display: grid;
            grid-template-columns: repeat(2, 1fr); /* 2 colonnes */
            gap: 10px; /* Espacement entre les éléments */
        }

        .grid-item {
            padding: 20px;
            text-align: center;
        }

        /* Media query pour passer à une seule colonne sur les petits écrans */
        @@media (max-width: 768px) {
            .grid-container {
                grid-template-columns: 1fr; /* 1 colonne pour les écrans plus petits */
            }
        }

        .image {
            border: 1px solid #555;
        }

        #container {
            height: 400px;
        }

        /* Optional: Fade effect for carousel */
        .carousel-item {
            transition: opacity 1s ease-in-out;
            opacity: 0;
        }

            .carousel-item.active {
                opacity: 1;
            }

        .carousel-item {
            transition: opacity 1s ease-in-out;
            opacity: 0;
        }

            .carousel-item.active {
                opacity: 1;
            }

        .nav-logo {
